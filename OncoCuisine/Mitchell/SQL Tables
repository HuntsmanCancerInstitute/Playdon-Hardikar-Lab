Trial 1:
CREATE TABLE IF NOT EXISTS "NUTRIENT_INFO" (
	"fdc_id"	INTEGER,
	"description"	TEXT,
	"Field3"	INTEGER,
	PRIMARY KEY("fdc_id")
);
CREATE TABLE IF NOT EXISTS "RECIPE_CATEGORIES" (
	"category_id"	INTEGER,
	"category_name"	TEXT NOT NULL UNIQUE,
	"description"	TEXT,
	"dietary_tags"	INTEGER,
	PRIMARY KEY("category_id" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "RECIPE_INFO_1" (
	"recipe_sku"	INTEGER,
	"recipe_name"	TEXT NOT NULL,
	"source_url"	TEXT,
	"source_name"	TEXT,
	"recipe_category"	TEXT,
	"cuisine"	TEXT,
	"prep_time_minutes"	INTEGER,
	"total_time_minutes"	INTEGER,
	"servings"	INTEGER,
	"image_url"	TEXT,
	"created_date"	TEXT,
	"updated_date"	TEXT,
	"preparation_instruction"	TEXT,
	PRIMARY KEY("recipe_sku" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "RECIPE_INGREDIENTS" (
	"recipe_sku"	INTEGER,
	"ingredient_name"	TEXT,
	"food_id"	TEXT,
	"quantity"	REAL,
	"measure"	TEXT,
	"description"	TEXT,
	"order_in_recipe"	INTEGER,
	"data_type"	TEXT,
	PRIMARY KEY("recipe_sku","ingredient_name"),
	FOREIGN KEY("food_id") REFERENCES "USDA_FOOD"("food_id"),
	FOREIGN KEY("recipe_sku") REFERENCES "RECIPE_INFO_1"("recipe_sku")
);
CREATE TABLE IF NOT EXISTS "RECIPE_NUTRITIONAL_SUMMARY" (
	"recipe_sku"	INTEGER,
	"calories_total"	REAL,
	"protein_total"	REAL,
	"fat_total"	REAL,
	"carb_total"	REAL,
	"fiber_g"	REAL,
	"sugar_g"	REAL,
	"sodium_mg"	REAL,
	"calcium_mg"	REAL,
	"allergen_warnings"	TEXT,
	PRIMARY KEY("recipe_sku"),
	FOREIGN KEY("recipe_sku") REFERENCES "RECIPE_INFO_1"("recipe_sku")
);
CREATE TABLE IF NOT EXISTS "SYMPTOM" (
	"symptom_id"	INTEGER,
	"symptom_name"	TEXT,
	"description"	TEXT,
	PRIMARY KEY("symptom_id" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "TREATMENT" (
	"treatment_id"	INTEGER,
	"Field2"	INTEGER,
	PRIMARY KEY("treatment_id")
);
CREATE TABLE IF NOT EXISTS "USDA_FOOD" (
	"food_id"	TEXT,
	"food_name"	TEXT,
	"food_group"	TEXT,
	"publication_date"	TEXT,
	PRIMARY KEY("food_id")
);
CREATE TABLE IF NOT EXISTS "USER_INFO" (
	"user_id"	INTEGER,
	"username"	TEXT NOT NULL,
	"email"	TEXT NOT NULL UNIQUE,
	"gender"	TEXT,
	"age"	INTEGER,
	"height"	REAL,
	"weight"	REAL,
	"dietary_restrictions"	TEXT,
	"medical_conditions"	TEXT,
	"preferences"	TEXT,
	"activity_level"	TEXT,
	"last_active"	TEXT,
	"cancer_type"	TEXT,
	"treatment_type"	TEXT,
	PRIMARY KEY("user_id" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "USER_RECIPES" (
	"user_id"	INTEGER,
	"recipe_sku"	INTEGER,
	"status"	TEXT,
	"rating"	REAL,
	"feedback"	TEXT,
	"personal_notes"	TEXT,
	PRIMARY KEY("user_id","recipe_sku"),
	FOREIGN KEY("recipe_sku") REFERENCES "RECIPE_INFO_1"("recipe_sku"),
	FOREIGN KEY("user_id") REFERENCES "USER_INFO"("user_id")
);
CREATE TABLE IF NOT EXISTS "USER_RECOMMENDATIONS" (
	"recommendation_id"	INTEGER,
	"user_id"	INTEGER NOT NULL,
	"recipe_sku"	INTEGER,
	"rationale"	TEXT,
	"recommendation_type"	TEXT,
	"ai_confidence_score"	REAL,
	"created_date"	TEXT,
	"viewed"	BOOLEAN DEFAULT 0,
	"accepted"	BOOLEAN DEFAULT 0,
	"feedback"	TEXT,
	"status"	TEXT DEFAULT 'pending' CHECK("status" IN ('pending', 'viewed', 'accepted', 'rejected')),
	"meal_type"	TEXT CHECK("meal_type" IN ('Breakfast', 'Lunch', 'Dinner', 'Snack')),
	PRIMARY KEY("recommendation_id" AUTOINCREMENT),
	FOREIGN KEY("recipe_sku") REFERENCES "RECIPE_INFO_1"("recipe_sku"),
	FOREIGN KEY("user_id") REFERENCES "USER_INFO"("user_id")
);


