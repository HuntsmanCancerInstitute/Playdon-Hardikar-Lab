TRIAL 2:

CREATE TABLE IF NOT EXISTS "FOOD_NUTRIENT" (
	"id"	INTEGER,
	"fdc_id"	INTEGER,
	"nutrient_id"	INTEGER,
	"amount"	REAL,
	PRIMARY KEY("id" AUTOINCREMENT),
	FOREIGN KEY("fdc_id") REFERENCES "USDA_FOOD"("fdc_id"),
	FOREIGN KEY("nutrient_id") REFERENCES "NUTRIENT_INFO"("nutrient_id")
);
CREATE TABLE IF NOT EXISTS "NUTRIENT_INFO" (
	"nutrient_id"	INTEGER,
	"name"	TEXT,
	"unit_name"	TEXT,
	PRIMARY KEY("nutrient_id")
);
CREATE TABLE IF NOT EXISTS "RECIPE_INFO_1" (
	"recipe_sku"	INTEGER,
	"recipe_name"	TEXT NOT NULL,
	"source_url"	TEXT,
	"source_name"	TEXT,
	"recipe_category"	TEXT,
	"cuisine"	TEXT,
	"prep_time_minutes"	INTEGER,
	"total_time_minutes"	INTEGER,
	"servings"	INTEGER,
	"image_url"	TEXT,
	"created_date"	TEXT,
	"updated_date"	TEXT,
	"preparation_instruction"	TEXT,
	PRIMARY KEY("recipe_sku" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "RECIPE_INGREDIENTS" (
	"recipe_sku"	INTEGER,
	"fdc_id"	INTEGER,
	"quantity"	REAL,
	"measure"	TEXT,
	"description"	TEXT,
	"order_in_recipe"	INTEGER,
	"data_type"	TEXT,
	PRIMARY KEY("recipe_sku","fdc_id"),
	FOREIGN KEY("fdc_id") REFERENCES "USDA_FOOD"("fdc_id"),
	FOREIGN KEY("recipe_sku") REFERENCES "RECIPE_INFO_1"("recipe_sku") ON DELETE CASCADE
);
CREATE TABLE IF NOT EXISTS "RECIPE_NUTRITIONAL_SUMMARY" (
	"recipe_sku"	INTEGER,
	"calories"	REAL,
	"protein"	REAL,
	"fat"	REAL,
	"carbohydrates"	REAL,
	"fiber"	REAL,
	"sugar"	REAL,
	PRIMARY KEY("recipe_sku"),
	FOREIGN KEY("recipe_sku") REFERENCES "RECIPE_INFO_1"("recipe_sku") ON DELETE CASCADE
);
CREATE TABLE IF NOT EXISTS "USDA_FOOD" (
	"fdc_id"	INTEGER,
	"description"	TEXT,
	"data_type"	TEXT,
	"food_category_id"	INTEGER,
	"publication_date"	TEXT,
	PRIMARY KEY("fdc_id")
);
CREATE TABLE IF NOT EXISTS "USER_INFO" (
	"user_id"	INTEGER,
	"username"	TEXT NOT NULL,
	"email"	TEXT NOT NULL UNIQUE,
	"gender"	TEXT,
	"age"	INTEGER,
	"height"	REAL,
	"weight"	REAL,
	"dietary_restrictions"	TEXT,
	"medical_conditions"	TEXT,
	"preferences"	TEXT,
	"activity_level"	TEXT,
	"last_active"	TEXT,
	PRIMARY KEY("user_id" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "USER_RECIPES" (
	"user_id"	INTEGER,
	"recipe_sku"	INTEGER,
	"status"	TEXT,
	"rating"	REAL,
	"feedback"	TEXT,
	PRIMARY KEY("user_id","recipe_sku"),
	FOREIGN KEY("recipe_sku") REFERENCES "RECIPE_INFO_1"("recipe_sku") ON DELETE CASCADE,
	FOREIGN KEY("user_id") REFERENCES "USER_INFO"("user_id") ON DELETE CASCADE
);
CREATE TABLE IF NOT EXISTS "USER_RECOMMENDATIONS" (
	"recommendation_id"	INTEGER,
	"user_id"	INTEGER NOT NULL,
	"recipe_sku"	INTEGER,
	"rationale"	TEXT,
	"recommendation_type"	TEXT,
	"ai_confidence_score"	REAL,
	"created_date"	TEXT,
	"viewed"	BOOLEAN DEFAULT 0,
	"accepted"	BOOLEAN DEFAULT 0,
	"feedback"	TEXT,
	"status"	TEXT,
	"meal_type"	TEXT CHECK("meal_type" IN ('Breakfast', 'Lunch', 'Dinner', 'Snack')),
	"source_model"	TEXT,
	PRIMARY KEY("recommendation_id" AUTOINCREMENT),
	FOREIGN KEY("recipe_sku") REFERENCES "RECIPE_INFO_1"("recipe_sku") ON DELETE CASCADE,
	FOREIGN KEY("user_id") REFERENCES "USER_INFO"("user_id") ON DELETE CASCADE
);
